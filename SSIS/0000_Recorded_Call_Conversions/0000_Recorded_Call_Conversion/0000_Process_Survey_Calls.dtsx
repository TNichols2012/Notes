<DTS:LoggingOptions><DTS:Property DTS:Name="LoggingMode">0</DTS:Property><DTS:Property DTS:Name="FilterKind">1</DTS:Property><DTS:Property DTS:Name="EventFilter" DTS:DataType="8"></DTS:Property></DTS:LoggingOptions><DTS:Property DTS:Name="ObjectName">04) Generate BAT file</DTS:Property><DTS:Property DTS:Name="DTSID">{DA696F85-C1BC-47CE-962C-D03338952646}</DTS:Property><DTS:Property DTS:Name="Description">ActiveX Script Task</DTS:Property><DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ActiveXScriptTask.ActiveXScriptTask, Microsoft.SqlServer.ActiveXScriptTask, Version=9.0.242.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property><DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><ActiveXScriptTask:ActiveXScriptTaskData ActiveXScriptTask:ScriptingLanguage="VBScript" ActiveXScriptTask:ScriptText="Dim obj_FSO, str_Data_File1, str_Data_File2, str_Data_File3, str_Data_File4&#xA;Dim str_SQLquery1&#xA;Dim str_SQLquery2&#xA;Dim str_SQLquery3 &#xA;Dim str_SQLquery4 &#xA;Dim str_DateString&#xA;&#xA;Dim rst_SQLquery1, rst_SQLquery_Check1&#xA;Dim rst_SQLquery2, rst_SQLquery_Check2&#xA;Dim rst_SQLquery3, rst_SQLquery_Check3&#xA;Dim rst_SQLquery4, rst_SQLquery_Check4&#xA;&#xA;Function Main()&#xA;&#xA;'CREATE THE DATESTRING&#xA;Dim str_Current_Date, str_Current_Month, str_Current_Month_Name, str_Current_Year&#xA;Dim str_Previous_Date, str_Previous_Month, str_Previous_Month_Name, str_Previous_Year&#xA;Dim str_Date&#xA;&#xA;'Daily Variables&#xA;Dim str_Daily_Month, str_Daily_Date, str_Daily_Year, str_Daily_Year_Abbr&#xA;&#xA;'Weekly Variables&#xA;Dim dt_SunDate, str_Weekly_Month, str_Weekly_Date, str_Weekly_Year&#xA;Dim int_Adjust_Day, int_Current_Day&#xA;&#xA;'Monthly Variables&#xA;Dim str_Monthly_Month,  str_Monthly_Date, str_Monthly_Year, str_Monthly_Year_Abbr&#xA;&#xA;	str_Current_Date = cstr ( Day ( Date ) )&#xA;	str_Previous_Date = cstr ( Day ( Date - 4 ) )&#xA;	str_Current_Month = cstr ( Month ( Date ) )&#xA;	str_Previous_Month = cstr ( Month ( Date - 4 ) )&#xA;	str_Current_Year =  cstr ( Year ( Date ) )&#xA;	str_Previous_Year =  cstr ( Year ( Date ) )&#xA;&#xA;	if ( len ( str_Current_Month ) = 1 ) then&#xA;		str_Current_Month = &quot;0&quot; &amp; str_Current_Month&#xA;	end if&#xA;&#xA;	if ( len ( str_Previous_Month ) = 1 ) then&#xA;		str_Previous_Month = &quot;0&quot; &amp; str_Previous_Month&#xA;	end if&#xA;&#xA;	if ( len ( str_Current_Date ) = 1 ) then&#xA;		str_Current_Date = &quot;0&quot; &amp; str_Current_Date&#xA;	end if&#xA;&#xA;	if ( len ( str_Previous_Date ) = 1 ) then&#xA;		str_Previous_Date = &quot;0&quot; &amp; str_Previous_Date&#xA;	end if&#xA;&#xA;	'str_Date = &quot;20080828&quot;&#xA;	str_Date = str_Previous_Year &amp; str_Previous_Month &amp; str_Previous_Date&#xA;&#xA;&#xA;&#xA;' THE MAIN FUNCTION&#xA;&#xA;Set obj_FSO = CreateObject(&quot;Scripting.FileSystemObject&quot;) &#xA;&#xA;If obj_FSO.FileExists(&quot;\\Usrc_fileserver\SQL_Server\Client_USMR\Work\USRCBR_Hourly_MP3_Conversion_A.bat&quot;) Then &#xA;	obj_FSO.DeleteFile &quot;\\Usrc_fileserver\SQL_Server\Client_USMR\Work\USRCBR_Hourly_MP3_Conversion_A.bat&quot;  &#xA;End If&#xA;&#xA;If obj_FSO.FileExists(&quot;\\Usrc_fileserver\SQL_Server\Client_USMR\Work\USRCBR_Hourly_MP3_Conversion_B.bat&quot;) Then &#xA;	obj_FSO.DeleteFile &quot;\\Usrc_fileserver\SQL_Server\Client_USMR\Work\USRCBR_Hourly_MP3_Conversion_B.bat&quot;  &#xA;End If&#xA;&#xA;If obj_FSO.FileExists(&quot;\\Usrc_fileserver\SQL_Server\Client_USMR\Work\USRCBR_Hourly_MP3_Conversion_C.bat&quot;) Then &#xA;	obj_FSO.DeleteFile &quot;\\Usrc_fileserver\SQL_Server\Client_USMR\Work\USRCBR_Hourly_MP3_Conversion_C.bat&quot;  &#xA;End If&#xA;&#xA;If obj_FSO.FileExists(&quot;\\Usrc_fileserver\SQL_Server\Client_USMR\Work\USRCBR_Hourly_MP3_Conversion_D.bat&quot;) Then &#xA;	obj_FSO.DeleteFile &quot;\\Usrc_fileserver\SQL_Server\Client_USMR\Work\USRCBR_Hourly_MP3_Conversion_D.bat&quot;  &#xA;End If&#xA;&#xA;Set str_Data_File1= obj_FSO.OpenTextFile(&quot;\\Usrc_fileserver\SQL_Server\Client_USMR\Work\USRCBR_Hourly_MP3_Conversion_A.bat&quot;, 8, True) &#xA;&#xA;	'Get list of files to be converted&#xA;	str_SQLquery1 =	&quot;SELECT TOP 1 (LEFT(RIGHT(CR.RecordingFileName, (27+LEN(CR.ContactNumber))), 8) ) AS Target_File_Name  &quot; &amp; _&#xA;			&quot;FROM dbo.CallResults CR  &quot; &amp; _&#xA;			&quot;INNER JOIN CallCenter.dbo.CallResultCodes CRC &quot;  &amp; _&#xA;			&quot;ON CR.AgentExitCode=CRC.CallResultCode  &quot; &amp; _&#xA;			&quot;WHERE CR.CallStartTime &gt;= dbo.Get_TheDate(GetDate()-5)  &quot; &amp; _&#xA;			&quot;AND CR.CallStartTime &lt;= dbo.Get_TheDate(GetDate()-2)  &quot; &amp; _&#xA;			&quot;AND LEN (CR.RecordingFileName) &gt; 1  &quot; &amp; _&#xA;			&quot;GROUP BY (LEFT(RIGHT(CR.RecordingFileName, (27+LEN(CR.ContactNumber))), 8) ) &quot; &amp; _&#xA;			&quot;ORDER BY (LEFT(RIGHT(CR.RecordingFileName, (27+LEN(CR.ContactNumber))), 8) ) &quot;&#xA;				&#xA;	'Get the ADO Recordset&#xA;	Set rst_SQLquery_Check1 = ExecuteSQL_Amcat(str_SQLquery1)&#xA;&#xA;	'If the Recordset is not empty then proceed&#xA;	str_Data_File1.WriteLine(&quot;dir&quot;)&#xA;	If Not (rst_SQLquery_Check1.Eof and rst_SQLquery_Check1.Bof ) Then 			&#xA;					&#xA;&#xA;			'ADO Recordset &#xA;			Set rst_SQLquery1 = ExecuteSQL_Amcat(str_SQLquery1)&#xA;&#xA;			'While Not rst_SQLquery1.Eof &#xA;				str_Data_File1.Write(&quot;del \\Usrc_fileserver\Amcat_Recordings\Sales_Calls\&quot;)&#xA;				'str_Data_File1.Write( rst_SQLquery1(&quot;Target_File_Name&quot;) )&#xA;&#xA;				str_Data_File1.Write( str_Date )&#xA;				str_Data_File1.Write(&quot;*.mp3&quot;)&#xA;				str_Data_File1.WriteLine&#xA;			&#xA;				'Move on to to the next record&#xA;			'	rst_SQLquery1.MoveNext		&#xA;			'Wend&#xA;&#xA;	Else	str_Data_File1.WriteLine(&quot;dir&quot;)&#xA;	End If &#xA;&#xA;	str_Data_File1.WriteLine(&quot;dir&quot;)&#xA;	str_Data_File1.Close&#xA;&#xA;&#xA;Set str_Data_File2= obj_FSO.OpenTextFile(&quot;\\Usrc_fileserver\SQL_Server\Client_USMR\Work\USRCBR_Hourly_MP3_Conversion_B.bat&quot;, 8, True) &#xA;&#xA;	'Get list of files to be converted&#xA;	str_SQLquery2 =	&quot;SELECT &quot; &amp;_&#xA;			&quot;SUBSTRING(RecordingsPath, 15, 50) AS Campaign_Folder &quot; &amp; _&#xA;			&quot;FROM tbl_Survey_Campaigns &quot; &amp; _&#xA;			&quot;WHERE LEN(SUBSTRING(RecordingsPath, 15, 50)) &gt; 1&quot; &#xA;				&#xA;	'Get the ADO Recordset&#xA;	Set rst_SQLquery_Check2 = ExecuteSQL(str_SQLquery2)&#xA;&#xA;&#xA;	'If the Recordset is not empty then proceed&#xA;	str_Data_File2.WriteLine(&quot;dir&quot;)&#xA;	If Not (rst_SQLquery_Check2.Eof and rst_SQLquery_Check2.Bof ) Then 			&#xA;					&#xA;			'ADO Recordset &#xA;			Set rst_SQLquery2 = ExecuteSQL(str_SQLquery2)&#xA;&#xA;&#xA;			If Not (rst_SQLquery_Check2.Eof and rst_SQLquery_Check2.Bof ) Then 			&#xA;				While Not rst_SQLquery2.Eof &#xA;					str_Data_File2.Write(&quot;IF NOT EXIST \\Usrcfileserver\Amcat_Recordings\Sales_Calls\&quot;)&#xA;					str_Data_File2.Write( rst_SQLquery2(&quot;Campaign_Folder&quot;) )&#xA;&#xA;					str_Data_File2.Write(&quot; MKDIR &quot;)&#xA;					str_Data_File2.Write(&quot;\\Usrcfileserver\Amcat_Recordings\Sales_Calls\&quot;)&#xA;					str_Data_File2.Write( rst_SQLquery2(&quot;Campaign_Folder&quot;) )&#xA;					str_Data_File2.WriteLine&#xA;			&#xA;					str_Data_File2.Write(&quot;DEL &quot;)&#xA;					str_Data_File2.Write(&quot;\\Usrcfileserver\Amcat_Recordings\Sales_Calls\&quot;)&#xA;					str_Data_File2.Write( rst_SQLquery2(&quot;Campaign_Folder&quot;) )&#xA;					str_Data_File2.Write(str_Date)&#xA;					'str_Data_File2.Write( rst_SQLquery1(&quot;Target_File_Name&quot;) )&#xA;					str_Data_File2.Write(&quot;*.mp3&quot;)&#xA;					str_Data_File2.WriteLine&#xA;&#xA;					str_Data_File2.Write(&quot;IF EXIST \\Usrc_dialer\Recordings\&quot;)&#xA;					str_Data_File2.Write( rst_SQLquery2(&quot;Campaign_Folder&quot;) )&#xA;					'str_Data_File2.WriteLine&#xA;			&#xA;					str_Data_File2.Write(&quot; DEL &quot;)&#xA;					str_Data_File2.Write(&quot;\\Usrc_dialer\Recordings\&quot;)&#xA;					str_Data_File2.Write( rst_SQLquery2(&quot;Campaign_Folder&quot;) )&#xA;					str_Data_File2.Write(str_Date)&#xA;					'str_Data_File2.Write( rst_SQLquery1(&quot;Target_File_Name&quot;) )&#xA;					str_Data_File2.Write(&quot;*.wav&quot;)&#xA;					str_Data_File2.WriteLine&#xA;&#xA;					'Move on to to the next record&#xA;					rst_SQLquery2.MoveNext		&#xA;				Wend&#xA;			End If&#xA;&#xA;&#xA;	Else	str_Data_File2.WriteLine(&quot;dir&quot;)&#xA;	End If &#xA;&#xA;	str_Data_File2.WriteLine(&quot;dir&quot;)&#xA;	str_Data_File2.Close&#xA;&#xA;&#xA;Set str_Data_File3= obj_FSO.OpenTextFile(&quot;\\Usrc_fileserver\SQL_Server\Client_USMR\Work\USRCBR_Hourly_MP3_Conversion_C.bat&quot;, 8, True) &#xA;&#xA;	'Get list of files to be converted&#xA;	str_SQLquery3 =	&quot;SELECT PKCallResults, Source_File_Name, Target_File_Name_2, MP3_File_Name_2 &quot; &amp; _&#xA;			&quot;FROM dbo.tbl_Survey_Calls &quot; &amp; _&#xA;			&quot;WHERE Converted IS NULL OR Converted IN ('No') &quot; &amp; _&#xA;			&quot;AND Call_Start_Time &gt; GetDate()-3 &quot; &amp; _&#xA;			&quot;ORDER BY Call_Start_Time &quot;&#xA;&#xA;	'Get the ADO Recordset&#xA;	Set rst_SQLquery_Check3 = ExecuteSQL(str_SQLquery3)&#xA;&#xA;	'If the Recordset is not empty then proceed&#xA;	str_Data_File3.WriteLine(&quot;dir&quot;)&#xA;	If Not (rst_SQLquery_Check3.Eof and rst_SQLquery_Check3.Bof ) Then 			&#xA;			&#xA;		'ADO Recordset &#xA;		Set rst_SQLquery3 = ExecuteSQL(str_SQLquery3)&#xA;&#xA;		While Not rst_SQLquery3.Eof &#xA;				str_Data_File3.Write(&quot;IF NOT EXIST &quot;)&#xA;				str_Data_File3.Write( rst_SQLquery3(&quot;MP3_File_Name_2&quot;) )&#xA;				str_Data_File3.Write( &quot; &quot; )&#xA;				str_Data_File3.Write( &quot;lame&quot;)&#xA;				str_Data_File3.Write( &quot; &quot; )&#xA;				str_Data_File3.Write( rst_SQLquery3(&quot;Source_File_Name&quot;) )&#xA;				str_Data_File3.Write( &quot; &quot; )&#xA;				str_Data_File3.Write( rst_SQLquery3(&quot;MP3_File_Name_2&quot;) )&#xA;				'str_Data_File.Write( &quot; &gt;&gt; c:\Lame_Test.log&quot; )&#xA;				str_Data_File3.WriteLine&#xA;&#xA;				str_Data_File3.Write(&quot;IF EXIST &quot;)&#xA;				str_Data_File3.Write( rst_SQLquery3(&quot;MP3_File_Name_2&quot;) )&#xA;				str_Data_File3.Write( &quot; &quot; )&#xA;				str_Data_File3.Write( &quot;del&quot;)&#xA;				str_Data_File3.Write( &quot; &quot; )&#xA;				str_Data_File3.Write( rst_SQLquery3(&quot;Source_File_Name&quot;) )&#xA;				str_Data_File3.WriteLine&#xA;&#xA;			'Move on to to the next record&#xA;			rst_SQLquery3.MoveNext		&#xA;&#xA;		Wend&#xA;&#xA;	Else	str_Data_File3.WriteLine(&quot;dir&quot;)&#xA;	End If &#xA;&#xA;	str_Data_File3.WriteLine(&quot;dir&quot;)&#xA;	str_Data_File3.Close&#xA;&#xA;End Function&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;Function ExecuteSQL(mySQLCmdText)&#xA;&#xA;	'ADO Connection String to the Inventory&#xA;	Const DB_CONNECT_STRING = &quot;Provider=SQLOLEDB.1;Data Source=USRC_FILESERVER; Initial Catalog=Data_Warehouse;user id = 'SQL_admin';password='AustinDat@'  &quot;&#xA;&#xA;	' instantiate the ADO objects&#xA;	  Dim myConn&#xA;	  Dim myRecordset&#xA;&#xA;	  set myConn = CreateObject(&quot;ADODB.Connection&quot;)&#xA;	  set myRecordset = CreateObject(&quot;ADODB.Recordset&quot; )&#xA;&#xA;	' set the connection properties to point to the Pubs database using the constant&#xA;	  myConn.Open = DB_CONNECT_STRING&#xA;	  myRecordset.Open mySQLCmdText, myConn&#xA;	  &#xA;	  Set ExecuteSQL = myRecordset&#xA;End Function		&#xA;&#xA;Function ExecuteSQL_Amcat(mySQLCmdText)&#xA;&#xA;	'ADO Connection String to the Inventory&#xA;	Const DB_CONNECT_STRING = &quot;Provider=SQLOLEDB.1;Data Source=USRC_AMCATSQL; Initial Catalog=CallCenterStats;user id = 'SQL_admin';password='AustinDat@'  &quot;&#xA;&#xA;	' instantiate the ADO objects&#xA;	  Dim myConn&#xA;	  Dim myRecordset&#xA;&#xA;	  set myConn = CreateObject(&quot;ADODB.Connection&quot;)&#xA;	  set myRecordset = CreateObject(&quot;ADODB.Recordset&quot; )&#xA;&#xA;	' set the connection properties to point to the database using the constant&#xA;	  myConn.Open = DB_CONNECT_STRING&#xA;	  myRecordset.Open mySQLCmdText, myConn&#xA;	  &#xA;	  Set ExecuteSQL_Amcat = myRecordset&#xA;End Function		&#xA;&#xA;Function Generate_DateString(Date)&#xA;Dim str_Current_Date, str_Current_Month, str_Current_Month_Name, str_Current_Year&#xA;Dim str_Previous_Date, str_Previous_Month, str_Previous_Month_Name, str_Previous_Year&#xA;&#xA;'Daily Variables&#xA;Dim str_Daily_Month, str_Daily_Date, str_Daily_Year, str_Daily_Year_Abbr&#xA;&#xA;'Weekly Variables&#xA;Dim dt_SunDate, str_Weekly_Month, str_Weekly_Date, str_Weekly_Year&#xA;Dim int_Adjust_Day, int_Current_Day&#xA;&#xA;'Monthly Variables&#xA;Dim str_Monthly_Month,  str_Monthly_Date, str_Monthly_Year, str_Monthly_Year_Abbr&#xA;&#xA;	str_Current_Date = cstr ( Day ( Date ) )&#xA;	str_Previous_Date = cstr ( Day ( Date - 4 ) )&#xA;	str_Current_Month = cstr ( Month ( Date ) )&#xA;	str_Previous_Month = cstr ( Month ( Date-4 ) )&#xA;	str_Current_Year =  cstr ( Year ( Date ) )&#xA;	str_Previous_Year =  cstr ( Year ( Date-4 ) )&#xA;&#xA;&#xA;	if ( len ( str_Current_Month ) = 1 ) then&#xA;		str_Current_Month = &quot;0&quot; &amp; str_Current_Month&#xA;	end if&#xA;&#xA;	if ( len ( str_Previous_Month ) = 1 ) then&#xA;		str_Previous_Month = &quot;0&quot; &amp; str_Previous_Month&#xA;	end if&#xA;&#xA;	if ( len ( str_Current_Date ) = 1 ) then&#xA;		str_Current_Date = &quot;0&quot; &amp; str_Current_Date&#xA;	end if&#xA;&#xA;	if ( len ( str_Previous_Date ) = 1 ) then&#xA;		str_Previous_Date = &quot;0&quot; &amp; str_Previous_Date&#xA;	end if&#xA;&#xA;'Set Generate_DateString = &quot;20080828&quot;&#xA;&#xA;Set Generate_DateString = str_Previous_Year '&amp; str_Previous_Month &amp; str_Previous_Date&#xA;&#xA;End Function&#xA;" ActiveXScriptTask:EntryMethod="Main" xmlns:ActiveXScriptTask="www.microsoft.com/sqlserver/dts/tasks/activexscripttask"/></DTS:ObjectData></DTS:Executable>
