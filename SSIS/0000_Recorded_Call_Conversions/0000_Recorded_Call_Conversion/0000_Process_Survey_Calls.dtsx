<property id="3" name="OpenRowset" dataType="System.String" state="default" isArray="false" description="Specifies the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="4" name="OpenRowsetVariable" dataType="System.String" state="default" isArray="false" description="Specifies the variable that contains the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="5" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=9.0.242.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">SELECT
CR.PKCallResults,
CR.ContactNumber AS Contact_Number,
CR.CallStartTime AS Call_Start_Time,
CRC.CallResultDescription AS Call_Disposition,
U.[Name] AS Sales_Rep,
DATEDIFF( s, CallSentToAgentTime, CallEndTime) AS Call_Seconds,
CR.RecordingFileName AS Orig_File_Name,

RTRIM('\\Usrc_dialer\Recordings\' + 
SUBSTRING(CR.RecordingFileName, 15, 
(LEN(CR.RecordingFileName)-( 15+ 
27+LEN(CR.ContactNumber)) ) ) + '\'+
RIGHT(CR.RecordingFileName, (27+LEN(CR.ContactNumber)))) 
AS Source_File_Name,

RTRIM('\\Usrcfileserver\Amcat_Recordings\Sales_Calls\' + 
LEFT(RIGHT(CR.RecordingFileName, 
(27+LEN(CR.ContactNumber))), 8) + '\' +
SUBSTRING(CR.RecordingFileName, 15, 
(LEN(CR.RecordingFileName)-( 15+ 
27+LEN(CR.ContactNumber)) ) ) + '\'+
RIGHT(CR.RecordingFileName, (27+LEN(CR.ContactNumber)))) 
AS Target_File_Name_1,

RTRIM('\\Usrcfileserver\Amcat_Recordings\Sales_Calls\' + 
SUBSTRING(CR.RecordingFileName, 15, 
(LEN(CR.RecordingFileName)-( 15+ 
27+LEN(CR.ContactNumber)) ) ) + '\'+
RIGHT(CR.RecordingFileName, (27+LEN(CR.ContactNumber)))) 
AS Target_File_Name_2,


RTRIM('\\Usrcfileserver\Amcat_Recordings\Sales_Calls' + 
SUBSTRING(CR.RecordingFileName, (LEN(CR.RecordingFileName)-(27+LEN(CR.ContactNumber)) ), (24+LEN(CR.ContactNumber)) ) + '.mp3') AS MP3_File_Name_1,

RTRIM('\\Usrcfileserver\Amcat_Recordings\Sales_Calls\' + 
SUBSTRING(CR.RecordingFileName, 15, 
(LEN(CR.RecordingFileName)-( 15+ 
27+LEN(CR.ContactNumber)) ) ) +
SUBSTRING(CR.RecordingFileName, (LEN(CR.RecordingFileName)-(27+LEN(CR.ContactNumber)) ), (24+LEN(CR.ContactNumber)) ) + '.mp3') AS MP3_File_Name_2,

(CASE WHEN CRC.CallResultDescription LIKE 'Sale%' OR CRC.CallResultDescription LIKE 'Pres%' OR CRC.CallResultDescription LIKE 'Cust Service%' OR CRC.CallResultDescription LIKE 'Gen Info%'THEN 'Sales' ELSE 'non-Sales' END) AS Transaction_Type
FROM dbo.CallResults CR
INNER JOIN CallCenter.dbo.CallResultCodes CRC
ON CR.AgentExitCode=CRC.CallResultCode
INNER JOIN CallCenter.dbo.[Users] U
ON U.Pkusers=CR.AgentFK


WHERE CR.CallStartTime &gt;= GetDate()-14
AND LEN (CR.RecordingFileName) &gt; 1

AND (LEN(CR.RecordingFileName)-( 15 + 27+LEN(CR.ContactNumber))) &gt; 0</property>
